{
  "identifier": "com.gunesozgur.homebrew",
  "compatibleVersion": 3,
  "bundleVersion": 1,
  "icon": "icon.png",
  "commands": [
    {
      "name": "Reload Plugins",
      "identifier": "homebrew.reload-plugins-identifier",
      "script": "reload-plugins.js"
    },
    {
      "name": "Reset Image Aspect Ratio",
      "identifier": "homebrew.reset-image-aspect-ratio-identifier",
      "script": "reset-image-aspect-ratio.js"
    },
    {
      "name": "Set Layout",
      "identifier": "homebrew.set-layout-identifier",
      "shortcut": "cmd ctrl l",
      "script": "set-layout.js"
    },
    {
      "name": "Select Unused Symbols",
      "identifier": "homebrew.select-unused-symbols-identifier",
      "script": "select-unused-symbols.js"
    },
    {
      "name": "Space Horizontally",
      "identifier": "homebrew.space-horizontally-identifier",
      "shortcut": "cmd ctrl h",
      "script": "space-horizontally.js"
    },
    {
      "name": "Space Vertically",
      "identifier": "homebrew.space-vertically-identifier",
      "shortcut": "cmd ctrl v",
      "script": "space-vertically.js"
    },
    {
      "name": "Zoom Info",
      "identifier": "homebrew.zoom-info-identifier",
      "shortcut": "cmd alt z",
      "script": "zoom-info.js"
    },
    {
      "name": "Zoom Tool",
      "identifier": "homebrew.zoom-tool-identifier",
      "shortcut": "cmd ctrl z",
      "script": "zoom-tool.js"
    }
  ],
  "menu": {
    "title": "Homebrew",
    "items": [
      "homebrew.reload-plugins-identifier",
      "homebrew.reset-image-aspect-ratio-identifier",
      "homebrew.set-layout-identifier",
      "homebrew.select-unused-symbols-identifier",
      "homebrew.space-vertically-identifier",
      "homebrew.space-horizontally-identifier",
      "homebrew.zoom-info-identifier",
      "homebrew.zoom-tool-identifier"
    ]
  },
  "version": "0.2.0",
  "description": "A collection of utilities.",
  "name": "Homebrew",
  "disableCocoaScriptPreprocessor": true,
  "appcast": "https://raw.githubusercontent.com/ozgurgunes/Sketch-Homebrew/master/.appcast.xml",
  "author": "Ozgur Gunes",
  "authorEmail": "o.gunes@gmail.com"
}